<script>
    var fechas, modal, utils, menu, superExcel, impresor, primordial;
    async function boilerplate(loadPrimordial=true){
        fechas = new Fechas();
        modal = new Modal();
        utils = new Utils();
        menu = new Menu();

        if(modal?.setAnimation && menu.animations != null) modal.setAnimation(menu.animations);
        
        if(loadPrimordial){
            try{
                let resp = await $.get({url: "/primordial"});
                //console.log(resp);
                primordial = resp;
                menu.setPermissions(primordial.permissions)
                menu.setExpiration(primordial?.expiration)
            }catch(err){
                alert(err.toString());
            }
        }
    }
</script>